# Environment configuration for ESG Management
# Rename this file to `.env` and fill in the values for your environment.

# Supabase configuration
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Optional Stripe publishable key used as a fallback when the edge
# function is unavailable.
VITE_STRIPE_PUBLISHABLE_KEY=

# Enable Tempo storybook routes during development
# VITE_TEMPO=true

# Use mock authentication instead of the real provider
# VITE_USE_MOCK_AUTH=true
